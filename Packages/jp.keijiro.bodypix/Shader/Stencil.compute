#pragma kernel PostprocessStencil

#include "Common.hlsl"

// Input
Texture2D<float> Input;
uint2 InputSize;

// Output
RWTexture2D<float> Output;

[numthreads(8, 8, 1)]
void PostprocessStencil(uint2 id : SV_DispatchThreadID)
{
    if (!all(id < InputSize)) return;

    Output[id] = Sigmoid(Input[id]);
}
